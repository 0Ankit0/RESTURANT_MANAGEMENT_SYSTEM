<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RMS_FRONTEND</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body >
   @await Html.PartialAsync("_Header")
 
  <div class="min-vh-100 d-flex flex-column">
        <main class="flex-grow-1">
            @RenderBody()
        </main>


    <footer>
        <section class="py-0 bg-1000 d-flex flex-column">
            <div class="container">
               
                <div class="row flex-center">
                    <div class="col-md-6 order-0 mt-2">
                        <p class="text-200 text-center text-md-center">All rights Reserved &copy; Your Company, 2021</p>
                    </div>
                   
                </div>
            </div>
        </section>
    </footer>
    </div>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
